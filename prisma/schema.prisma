generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Room {
  id        String   @id @default(uuid())
  createdAt DateTime @default(now())
  expiresAt DateTime
  isActive  Boolean  @default(true)
  
  @@map("rooms")
}
